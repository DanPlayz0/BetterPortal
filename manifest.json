{

    "manifest_version": 2,
    "name": "Better Portal",
    "version": "1.0",
  
    "description": "Improvements to Geffen Academy Portal",
  
    "content_scripts": [
      {
        "matches": ["https://geffenacademy.myschoolapp.com/*"],
        "js": ["betterPortal.js"],
	"all_frames": true
      }
    ],

    "browser_specific_settings": {
        "gecko": {
          "id": "better-portal@gamingdoom.net",
          "strict_min_version": "1.0"
        }
      },

    "permissions": [
        "activeTab"
      ]
  }